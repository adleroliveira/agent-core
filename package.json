{
  "name": "agent-core",
  "version": "0.4.4",
  "description": "",
  "main": "dist/sdk/index.js",
  "types": "dist/sdk/index.d.ts",
  "scripts": {
    "prebuild": "pnpm generate:openapi && pnpm generate:api-client",
    "build": "cd frontend && pnpm build && cd .. && nest build",
    "start": "nest start",
    "start:dev": "nest start --watch --preserveWatchOutput",
    "test": "echo \"Error: no test specified\" && exit 1",
    "frontend:dev": "cd frontend && pnpm dev",
    "frontend:build": "cd frontend && pnpm build",
    "frontend:preview": "cd frontend && pnpm preview",
    "generate:openapi": "nest build && node -e \"const { NestFactory } = require('@nestjs/core'); const { AppModule } = require('./dist/app.module'); const { DocumentBuilder, SwaggerModule } = require('@nestjs/swagger'); const fs = require('fs'); const path = require('path'); async function generateOpenApi() { const app = await NestFactory.create(AppModule); app.setGlobalPrefix('api'); const config = new DocumentBuilder().setTitle('Agent Core API').setDescription('The Agent Core API description').setVersion('1.0').addTag('agents').build(); const document = SwaggerModule.createDocument(app, config); fs.writeFileSync(path.resolve('./openapi.json'), JSON.stringify(document, null, 2)); await app.close(); } generateOpenApi();\"",
    "generate:api-client": "npx openapi-typescript-codegen --input ./openapi.json --output ./frontend/src/api-client",
    "dev": "concurrently \"pnpm start:dev\" \"pnpm frontend:dev\"",
    "migration:run": "ts-node -r tsconfig-paths/register src/adapters/storage/typeorm/run-migrations.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-bedrock": "^3.763.0",
    "@aws-sdk/client-bedrock-runtime": "^3.758.0",
    "@nestjs/common": "^11.0.11",
    "@nestjs/config": "^4.0.1",
    "@nestjs/core": "^11.0.11",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/platform-express": "^11.0.11",
    "@nestjs/swagger": "^11.1.0",
    "@nestjs/typeorm": "^11.0.0",
    "@types/prismjs": "^1.26.5",
    "@types/uuid": "^10.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.7",
    "duck-duck-scrape": "^2.2.7",
    "markdown-to-jsx": "^7.7.4",
    "prismjs": "^1.30.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.21",
    "uuid": "^11.1.0",
    "vectra": "^0.9.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.6",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.0.11",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.10",
    "@types/supertest": "^6.0.2",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "openapi-typescript-codegen": "^0.29.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "@nestjs/core",
      "esbuild"
    ],
    "onlyBuiltDependencies": [
      "@nestjs/core",
      "@scarf/scarf",
      "sqlite3"
    ]
  }
}